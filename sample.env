# Supabase Configuration (supports both naming conventions)
# For manual setup, use NEXT_PUBLIC_ prefixed variables
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# For Vercel/Supabase integration, these are auto-generated
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_JWT_SECRET=

# Database Configuration
# For local development
DATABASE_URL=postgresql://postgres:password@localhost:5432/dbname
DIRECT_URL=postgresql://postgres:password@localhost:5432/dbname

# For Vercel Postgres Integration (auto-generated in production)
# These are automatically set by Vercel when you connect Supabase
POSTGRES_PRISMA_URL=
POSTGRES_URL_NON_POOLING=
POSTGRES_URL=
POSTGRES_USER=
POSTGRES_HOST=
POSTGRES_PASSWORD=
POSTGRES_DATABASE=

# Stripe Configuration
STRIPE_SECRET_KEY=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=

# Domain Configuration
NEXT_PUBLIC_DOMAIN=http://localhost:3000